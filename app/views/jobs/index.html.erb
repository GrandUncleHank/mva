


 <% if user_signed_in? %>
<div class="fixed-action-btn">
    <a class="btn-floating btn-large red">
      <i class="large material-icons">add</i>
    </a>
  </div>
  

<% end %>

  
  
    <div class="row">
      <div class="col s12">
      <% if user_signed_in? %>
<%= link_to "Create Post", new_job_path, class: "btn  light-blue darken-3" %>
<% end %>
<% @jobs.each do |job| %>
<div class="card-panel whitecolor">
  
<h3><%= link_to job.title, job_path(job), class: "card-title " %></h3>
<p><%= truncate(job.description) %></p>
<p><%= job.contact %></p>
<p><%= job.created_at %></p>

</div>

<% end %>
</div>
</div>


