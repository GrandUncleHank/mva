<h1 class="jobtitle">Jobs</h1>
<div class="card-panel">
    <div class="row">
      <div class="col s12 m5">
    
<% @jobs.each do |job| %>
<div class="card-panel">
  
<h3><%= link_to job.title, job_path(job), class: "card-title " %></h3>
<p><%= job.description %></p>
<p><%= job.contact %></p>

</div>

<% end %>
</div>
</div>


</div>

<% if user_signed_in? %>
<%= link_to "Create Post", new_job_path, class: "btn  light-blue darken-3" %>
<% end %>